class Animal
{
    void eat()
    {
        System.out.println("eating...");
    }
}
class Dog extends Animal
{
    void bark()
    {
         System.out.println("barking...");
    }
}
class BabyDog extends Dog
{
    void weep()
    {
        System.out.println("weeping...");
    }
}
class Example
{
    public static void main(String[]args)
    {
        BabyDog ob=new BabyDog();
        ob.weep();
        ob.bark();
        ob.eat();
    }
}
