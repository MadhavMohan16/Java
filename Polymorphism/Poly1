class A
{
    public void show()
    {
        System.out.println("Hello");
    }
    public void show(int i)
    {
        System.out.println("Hello "+i);
    }
}
class Poly1
{
    public static void main(String[]args)
    {
        A ob=new A();
        ob.show(5);//compiletime polymorphism,early binding,static binding,methodoverloading
    }
}
        
