class A1
{
    public void show()
    {
        System.out.println("In class A1");
    }
}
class B1 extends A1
{
    public void show()
    {
        System.out.println("In class B1");
    }
}
class C1 extends B1
{
    public void show()
    {
        System.out.println("In class C1");
    }
}
class Poly4
{
    public static void main(String[]args)
    {
        A1 ob;
        ob=new B1();
        ob.show();
        ob=new C1();
        ob.show();
    }
}
