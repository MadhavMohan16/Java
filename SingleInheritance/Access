class A1
{
    int i;
    private int j;
    void setij(int x,int y)
    {
        i=x;
        j=y;
    }
}
class B1 extends A1
{
    int total;
    void sum()
    {
        total=i+j;
    }
}
class Access
{
    public static void main(String[]args)
    {
        B1 ob=new B1();
        ob.setij(10,12);
        ob.sum();
        System.out.println("Total is="+ob.total);
    }
}
