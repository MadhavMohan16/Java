public class StringjoinMethod
{
    public static void main(String[]args)
    {
        String s1="Sachin";
        String s2="Tendulkar";
        String s3=String.join("-","Welcome","To","Javatpoint");
        System.out.println(String.join(",",s1,s2));
        System.out.println(String.join(";",s1,s2));
        //System.out.println(String.join(null,s1,s2));
        System.out.println(String.join(",",null,s2));
        System.out.println(s3);
    }
    
}
