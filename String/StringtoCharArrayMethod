public class StringtoCharArrayMethod
{
    public static void main(String[]args)
    {
        String s="Computer";
        char []ch=s.toCharArray();
        for(int i=0;i<ch.length;i++)
        {
        System.out.print(ch[i]);
        }
             /*OR*/
        /*System.out.println(ch);*/
    }
}
